    "WorkStation": {
      "Type": "AWS::EC2::Instance",
      "DependsOn" : ["ChefServer","AutomateServer","ChefCompliance","Splunk","Build01","Build02","Build03","Acceptance","Union","Rehearsal","Delivered"],
      "Properties": {
        "InstanceType": "t2.small",
